{
  "name": "jnc-mis-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^4.21.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start:production": "env-cmd -f .env.production react-scripts start",
    "start:local": "env-cmd -f .env.local react-scripts start",
    "start:dev": "env-cmd -f .env.dev react-scripts start",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "build:local": "env-cmd -f .env.local react-scripts build",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "test:production": "env-cmd -f .env.production react-scripts test",
    "test:local": "env-cmd -f .env.local react-scripts test",
    "test:dev": "env-cmd -f .env.dev react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^8.19.0"
  }
